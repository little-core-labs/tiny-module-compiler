{
  "name": "tiny-module-compiler",
  "version": "1.3.0",
  "description": "Compile, archive, unpack, and load compiled modules leveraging v8 cached data.",
  "main": "index.js",
  "scripts": {
    "test": "nyc -x fixtures -x example.js -x bin -x tmp -x messages.js -x test tape test/*.js",
    "clean": "scripts/clean"
  },
  "bin": {
    "tmc": "bin/tmc"
  },
  "files": [
    "*.js",
    "bin/tmc"
  ],
  "keywords": [
    "compile",
    "v8",
    "modules",
    "archive",
    "cache",
    "tiny",
    "box",
    "load"
